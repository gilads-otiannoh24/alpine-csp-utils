{
  "name": "@gilads-otiannoh24/cspine",
  "version": "0.0.8",
  "description": "A set of alpinejs utility functions for working with alpinejs Content Security Policy (CSP) build easier",
  "types": "dist/utils.d.ts",
  "keywords": [
    "alpinejs",
    "csp",
    "utils"
  ],
  "main": "./dist/module.js",
  "module": "./dist/module.mjs",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.js",
      "types": "./dist/types/CSPine.d.ts"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "author": "Ian Ochieng",
  "license": "MIT",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "vitest",
    "types": "tsc --project tsconfig.json",
    "dev": "webpack --watch",
    "build": "webpack && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gilads-otiannoh24/alpine-csp-utils"
  },
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/gilads-otiannoh24/alpine-csp-utils/issues"
  },
  "homepage": "https://github.com/gilads-otiannoh24/alpine-csp-utils#readme",
  "devDependencies": {
    "@types/alpinejs": "^3.13.11",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.17",
    "jsdom": "^26.1.0",
    "ts-loader": "^9.5.2",
    "vitest": "^3.2.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@alpinejs/csp": "^3.14.9",
    "alpinejs": "^3.14.9",
    "lodash": "^4.17.21"
  }
}
